@{
}
@model TP3.Models.Cadeteria

<h1>Empleados de @Model.Nombre</h1>

<table class="table">
    <thead>
        <tr>
            <th scope="col">Id</th>
            <th scope="col">Nombre</th>
            <th scope="col">Direccion</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in @Model.ListadoCadetes)
        {

            <tr>
                <th scope="row">@item.Id</th>
                <td>@item.Nombre</td>
                <td>@item.Direccion</td>
            </tr>

        }
    </tbody>
    </table>